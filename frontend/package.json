{
  "name": "@importcsv/react",
  "version": "0.2.0",
  "description": "Open-source CSV and XLS/XLSX file importer for React and JavaScript",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishekray07/importcsv.git"
  },
  "keywords": [
    "csv",
    "import",
    "excel",
    "data",
    "importer",
    "react",
    "csv-import"
  ],
  "author": "ImportCSV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhishekray07/importcsv/issues"
  },
  "homepage": "https://github.com/abhishekray07/importcsv#readme",
  "types": "build/index.d.ts",
  "scripts": {
    "build:js": "rollup --config rollup.config-js.js && cp package-js.json build/package.json",
    "build:react": "vite build",
    "build:react:dev": "vite build --mode development",
    "build:react:prod": "vite build --mode production",
    "publish:local:js": "yarn build:js && cd build && yalc publish",
    "publish:local:react": "yarn build:react:dev && yalc publish",
    "publish:local:react:prod": "yarn build:react:prod && yalc publish",
    "publish:js": "yarn build:js && cd build && npm publish",
    "publish:react": "NPM_PACKAGE_BUILD=true yarn build:react:prod && yarn publish --access public",
    "build:watch": "vite build --watch --mode development",
    "dev:examples": "cd examples && npm install && npm run dev"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "optionalDependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/babel__core": "^7.20.1",
    "@types/jest": "^24.9.1",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/use-sync-external-store": "0.0.3",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^5.0.1",
    "core-js": "^3.22.7",
    "css-loader": "^6.8.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "postcss-import": "^16.1.1",
    "prettier": "^2.8.6",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.5.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "use-sync-external-store": "^1.2.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toast": "^1.2.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.507.0",
    "papaparse": "^5.4.1",
    "postcss": "^8.5.3",
    "react-dropzone": "^14.2.3",
    "react-frame-component": "^5.2.7",
    "react-hotkeys-hook": "^5.1.0",
    "tailwind-merge": "^3.2.0",
    "zustand": "^4.4.7"
  },
  "type": "module"
}
