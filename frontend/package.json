{
  "name": "@importcsv/react",
  "version": "0.1.11",
  "description": "Open-source CSV and XLS/XLSX file importer for React and JavaScript",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishekray07/importcsv.git"
  },
  "keywords": [
    "csv",
    "import",
    "excel",
    "data",
    "importer",
    "react",
    "csv-import"
